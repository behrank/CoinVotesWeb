{
  "extends": "next/core-web-vitals",
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "react/no-unescaped-entities": ["error", {
      "forbid": [{
        "char": ">",
        "alternatives": ["&gt;"]
      }, {
        "char": "}",
        "alternatives": ["&#125;"]
      }]
    }],
    "@typescript-eslint/no-empty-interface": "warn"
  }
} 